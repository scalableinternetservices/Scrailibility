<html lang="en">

<head>
    <meta charset="utf-8">
    
   
    <title>edit</title>

    

    <!-- Fonts -->
    

    <link href='http://fonts.googleapis.com/css?family=Orbitron' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Raleway:400,500,300' rel='stylesheet' type='text/css'> 

    
<body id="page-top" data-spy="scroll" data-target=".navbar-custom">
<p id="notice"><%= notice %></p>

<h2>Listing Profiles</h2>

<div class="table-show">
<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>
<div class="render">
<%= link_to 'New Profile', new_profile_path %>
</dic>
</body>
